# H stands for Hebrew, and its capital so it doesnt interfere with other combos
# the abcs are the positions the letters take in the hebrew keyboard
# this is the qwerty version:
<Multi_key> <H> <t>        :   ""    U05D0 # HEBREW LETTER ALEF
<Multi_key> <H> <c>        :   ""    U05D1 # HEBREW LETTER BET
<Multi_key> <H> <d>        :   ""    U05D2 # HEBREW LETTER GIMEL
<Multi_key> <H> <s>        :   ""    U05D3 # HEBREW LETTER DALET
<Multi_key> <H> <v>        :   ""    U05D4 # HEBREW LETTER HE
<Multi_key> <H> <u>        :   ""    U05D5 # HEBREW LETTER VAV
<Multi_key> <H> <z>        :   ""    U05D6 # HEBREW LETTER ZAYIN
<Multi_key> <H> <j>        :   ""    U05D7 # HEBREW LETTER HET
<Multi_key> <H> <y>        :   ""    U05D8 # HEBREW LETTER TET
<Multi_key> <H> <h>        :   ""    U05D9 # HEBREW LETTER YOD
<Multi_key> <H> <f>        :   ""    U05DB # HEBREW LETTER KAF
<Multi_key> <H> <l>        :   ""    U05DA # HEBREW LETTER FINAL KAF
<Multi_key> <H> <k>        :   ""    U05DC # HEBREW LETTER LAMED
<Multi_key> <H> <n>        :   ""    U05DE # HEBREW LETTER MEM
<Multi_key> <H> <o>        :   ""    U05DD # HEBREW LETTER FINAL MEM
<Multi_key> <H> <b>        :   ""    U05E0 # HEBREW LETTER NUN
<Multi_key> <H> <i>        :   ""    U05DF # HEBREW LETTER FINAL NUN
<Multi_key> <H> <x>        :   "住"    U05E1 # HEBREW LETTER SAMEKH
<Multi_key> <H> <g>        :   "注"    U05E2 # HEBREW LETTER AYIN
<Multi_key> <H> <p>        :   "驻"    U05E4 # HEBREW LETTER PE
<Multi_key> <H> <semicolon>        :   "祝"    U05E3 # HEBREW LETTER FINAL PE
<Multi_key> <H> <m>        :   "爪"    U05E6 # HEBREW LETTER TSADI
<Multi_key> <H> <period>        :   "抓"    U05E5 # HEBREW LETTER FINAL TSADI
<Multi_key> <H> <e>        :   "拽"    U05E7 # HEBREW LETTER QOF
<Multi_key> <H> <r>        :   "专"    U05E8 # HEBREW LETTER RESH
<Multi_key> <H> <a>        :   "砖"    U05E9 # HEBREW LETTER SHIN
<Multi_key> <H> <comma>        :   "转"    U05EA # HEBREW LETTER TAV

