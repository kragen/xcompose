# H stands for Hebrew, and its capital so it doesnt interfere with other combos
# the abcs are the positions the letters take in the hebrew keyboard
# this is the qwerty version:
<Multi_key> <H> <t>        :   "א"    U05D0 # HEBREW LETTER ALEF
<Multi_key> <H> <c>        :   "ב"    U05D1 # HEBREW LETTER BET
<Multi_key> <H> <d>        :   "ג"    U05D2 # HEBREW LETTER GIMEL
<Multi_key> <H> <s>        :   "ד"    U05D3 # HEBREW LETTER DALET
<Multi_key> <H> <v>        :   "ה"    U05D4 # HEBREW LETTER HE
<Multi_key> <H> <u>        :   "ו"    U05D5 # HEBREW LETTER VAV
<Multi_key> <H> <z>        :   "ז"    U05D6 # HEBREW LETTER ZAYIN
<Multi_key> <H> <j>        :   "ח"    U05D7 # HEBREW LETTER HET
<Multi_key> <H> <y>        :   "ט"    U05D8 # HEBREW LETTER TET
<Multi_key> <H> <h>        :   "י"    U05D9 # HEBREW LETTER YOD
<Multi_key> <H> <f>        :   "כ"    U05DB # HEBREW LETTER KAF
<Multi_key> <H> <l>        :   "ך"    U05DA # HEBREW LETTER FINAL KAF
<Multi_key> <H> <k>        :   "ל"    U05DC # HEBREW LETTER LAMED
<Multi_key> <H> <n>        :   "מ"    U05DE # HEBREW LETTER MEM
<Multi_key> <H> <o>        :   "ם"    U05DD # HEBREW LETTER FINAL MEM
<Multi_key> <H> <b>        :   "נ"    U05E0 # HEBREW LETTER NUN
<Multi_key> <H> <i>        :   "ן"    U05DF # HEBREW LETTER FINAL NUN
<Multi_key> <H> <x>        :   "ס"    U05E1 # HEBREW LETTER SAMEKH
<Multi_key> <H> <g>        :   "ע"    U05E2 # HEBREW LETTER AYIN
<Multi_key> <H> <p>        :   "פ"    U05E4 # HEBREW LETTER PE
<Multi_key> <H> <semicolon>        :   "ף"    U05E3 # HEBREW LETTER FINAL PE
<Multi_key> <H> <m>        :   "צ"    U05E6 # HEBREW LETTER TSADI
<Multi_key> <H> <period>        :   "ץ"    U05E5 # HEBREW LETTER FINAL TSADI
<Multi_key> <H> <e>        :   "ק"    U05E7 # HEBREW LETTER QOF
<Multi_key> <H> <r>        :   "ר"    U05E8 # HEBREW LETTER RESH
<Multi_key> <H> <a>        :   "ש"    U05E9 # HEBREW LETTER SHIN
<Multi_key> <H> <comma>        :   "ת"    U05EA # HEBREW LETTER TAV

